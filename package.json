{
  "name": "fpsmember",
  "build": {
    "appId": "com.quadb.fpsmember",
    "artifactName": "${productName}.${ext}",
    "copyright": "copyright Â© 2021 Quad B Software",
    "productName": "FPSMember",
    "win": {
      "target": [
        "portable",
        "nsis",
        "zip"
      ],
      "compression": "maximum",
      "publisherName": "Quad B Software",
      "signingHashAlgorithms": [
        "sha1"
      ]
    },
    "nsis": {
      "artifactName": "${productName}-Setup.${ext}",
      "oneClick": true,
      "perMachine": false,
      "allowToChangeInstallationDirectory": false
    },
    "linux": {
      "target": [
        "AppImage"
      ],
      "category": "Network",
      "compression": "maximum"
    }
  },
  "version": "1.7.6",
  "description": "FPSMember Windows App",
  "main": "main.js",
  "scripts": {
    "test": "electron .",
    "postinstall": "electron-builder install-app-deps",
    "build": "electron-builder --windows --linux",
    "release": "electron-builder --windows --linux --publish always"
  },
  "keywords": [
    "FPSThailand",
    "FPSMember",
    "MiTH"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "electron": "^12.0.6",
    "electron-builder": "^22.11.1"
  },
  "dependencies": {
    "custom-electron-titlebar": "^3.2.6",
    "discord-rpc": "^3.2.0",
    "electron-updater": "^4.3.9",
    "nucleus-nodejs": "^3.0.8"
  }
}
