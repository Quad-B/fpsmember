{
  "name": "fpsmember",
  "build": {
    "appId": "com.boyphongsakorn.fpsmember",
    "artifactName": "${productName}.${ext}",
    "copyright": "copyright Â© 2021 Quad B Software",
    "productName": "FPSMember",
    "win": {
      "target": [
        "portable"
      ],
      "compression": "maximum",
      "publisherName": "Quad B Software",
      "signingHashAlgorithms": [
        "sha1"
      ]
    },
    "linux": {
      "target": [
        "AppImage"
      ],
      "category": "Network",
      "compression": "maximum"
    }
  },
  "version": "1.5.1",
  "description": "FPSMember Windows App",
  "main": "main.js",
  "scripts": {
    "test": "electron .",
    "postinstall": "electron-builder install-app-deps",
    "build": "electron-builder --windows",
    "release": "electron-builder --windows --publish always"
  },
  "keywords": [
    "FPSThailand"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "electron": "^12.0.1",
    "electron-builder": "^22.10.5"
  },
  "dependencies": {
    "custom-electron-titlebar": "^3.2.6",
    "nucleus-nodejs": "^3.0.8"
  }
}
